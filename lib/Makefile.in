@SET_MAKE@

top_builddir=@top_builddir@
top_srcdir=@top_srcdir@
srcdir=@srcdir@
VPATH=@srcdir@

include ../Make.rules

LIB_DEPS=@LIB_DEPS@
LIB_OBJS=@LIB_OBJS@

"":

dummy:

all:
	@echo "Running make from top level directory."
	cd ../ && $(MAKE) all

Makefile: Makefile.in ../config.status
	cd ../ && ./config.status

.c.o:
	$(LIBTOOL) --mode=compile --tag=CC $(CC) $(CPPFLAGS) $(CFLAGS) -c $<

libltdl: dummy
	cd libltdl/ && $(MAKE)

prbase.a: $(LIB_OBJS)
	$(AR) rc prbase.a $(patsubst %.o,.libs/%.o,$(LIB_OBJS))
	$(RANLIB) prbase.a

lib: prbase.a $(LIB_DEPS)

install:

clean:
	$(RM) *.o prbase.a
	$(LIBTOOL) --mode=clean $(RM) `echo $(LIB_OBJS) | sed 's/\.o$\/.lo/g'`
	test -z $(LIB_DEPS) || (cd libltdl/ && $(MAKE) clean)

depend:
	$(MAKEDEPEND) $(CPPFLAGS) $(top_srcdir)/lib/*.c 2>/dev/null
	$(MAKEDEPEND) $(CPPFLAGS) -fMakefile.in $(top_srcdir)/lib/*.c 2>/dev/null

distclean: clean
	-$(RM) *.gcda *.gcno
	test -z $(LIB_DEPS) || (cd libltdl/ && $(MAKE) distclean)
